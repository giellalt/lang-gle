                ! Add "a"
+Sg:^Leaa   Gen-sg-initial;


LEXICON Gen-sg-D4       ! 4th. Declension - No change
+Sg:0   Gen-sg-initial;

LEXICON Gen-sg-D5-1     ! 5th. Declension - (Broaden) + ach
