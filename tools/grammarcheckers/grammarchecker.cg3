LIST @PPRED = @PPRED ;        #!! @PPRED
LIST @APP = @APP ;			   #!! `@APP      `
LIST @APP-N< = @APP-N< ;	   #!! `@APP-N<   `
LIST @APP-Pron< = @APP-Pron< ; #!! `@APP-Pron<`
