LIST @HAB> = @HAB> ;                    #!! `@HAB>`
LIST @<HAB = @<HAB ;                    #!! `@<HAB`
SET @HAB = @<HAB OR @HAB> ;
LIST @>N = @>N ;		      #!! `@>N`
